    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.VisualizaAlbumActivity" />
        <activity android:name=".teste" />
        <activity android:name=".activities.VisualizaArtistaActivity" />
        <activity android:name=".activities.VisualizaMusicasAlbumActivity" />
        <activity android:name=".activities.VisualizaAlbunsArtistaActivity" />
        <activity android:name=".activities.PlayMusicaActivity" />
        <activity android:name=".activities.BuscaMusicaActivity" />
        <activity android:name=".activities.BuscaAlbumActivity" />
        <activity android:name=".activities.BuscaArtistaActivity"></activity>
    </application>

</manifest>